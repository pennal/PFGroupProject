<div id="{{ resultsNumber }}" class="weatherBox">
    <div class="basicInfo">
        <p class="dayName">{{ dayName }} {{ dayNumber }}</p>
        <div class="forecast">
            <!-- NEED TO BE ENCODED IN A CERTAIN WAY -->
            {{ forecastCode }}
        </div>
        <div class="degree">
            {{ mainTemp }}° C
        </div>
    </div>
    <div class="expandedInfo hidden">
        <table>

            <tr>
                <td>Humidity</td>
                <td>{{ humidity }}</td>
            </tr>
            <tr>
                <td>Pressure</td>
                <td>{{ pressure }}</td>
            </tr>
            <tr>
                <td>Wind Speed</td>
                <td>{{ windSpeed }}</td>
            </tr>
            <tr>
                <td>Day</td>
                <td>{{ dayTemp }}° C</td>
            </tr>
            <tr>
                <td>Evening</td>
                <td>{{ eveningTemp }}° C</td>
            </tr>
            <tr>
                <td>Sunrise</td>
                <td>{{ Sunrise }}</td>
            </tr>
            <tr>
                <td>Sunset</td>
                <td>{{ Sunset }}</td>
            </tr>
            <tr>
                <td>Max</td>
                <td>{{ maxTemp }}° C</td>
            </tr>
            <tr>
                <td>Min</td>
                <td>{{ minTemp }}° C</td>
            </tr>
            <tr>
                <td>Morning</td>
                <td>{{ morningTemp }}° C</td>
            </tr>
            <tr>
                <td>Night</td>
                <td>{{ nightTemp }}° C</td>
            </tr>

        </table>
    </div>
</div>